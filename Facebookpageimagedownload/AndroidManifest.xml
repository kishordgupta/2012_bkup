<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.lialit.facebookpagefeedlite"
    android:versionCode="4"
    android:versionName="1.3" >

    <uses-sdk
        android:minSdkVersion="5"
        android:targetSdkVersion="5" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
       <uses-permission android:name="android.permission.SET_WALLPAPER"/>
   <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <application
        android:name="com.lialit.facebookpagefeedlite.UILApplication"
        android:icon="@drawable/app_icon"
        android:label="@string/app_name" >
        <activity
            android:name="com.lialit.facebookpagefeedlite.HomeActivity"
            android:label="@string/app_name" 
            android:screenOrientation="portrait" >
    
        </activity>
      
        <activity
            android:name="com.lialit.facebookpagefeedlite.CategoryGridActivity"
            android:label="@string/ac_name_image_grid"
             android:screenOrientation="portrait"  />
       
        <activity
            android:name="com.lialit.facebookpagefeedlite.ImagePagerActivity"
            android:label="@string/ac_name_image_pager"
             android:screenOrientation="portrait"  />
    
               <activity
            android:name="com.peakcoders.backgroundTasks.Adderparser"
            android:label="@string/ac_name_image_gallery" 
             android:screenOrientation="portrait"/>
                <activity
            android:name="com.peakcoders.backgroundTasks.killerparser"
            android:label="@string/ac_name_image_gallery" 
             android:screenOrientation="portrait"/>
                   <activity
            android:name="com.peakcoders.backgroundTasks.StackParser"
            android:label="@string/ac_name_image_gallery"
             android:screenOrientation="portrait" />
              <activity
            android:name="com.lialit.facebookpagefeedlite.LoginActivity"
            android:label="@string/ac_name_image_gallery"
             android:screenOrientation="portrait" >        <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter></activity>
  <activity android:name="com.google.ads.AdActivity"
        android:theme="@android:style/Theme.NoTitleBar.Fullscreen"
        android:configChanges="keyboard|keyboardHidden|orientation|screenLayout|uiMode|screenSize|smallestScreenSize"  />
      
    </application>

</manifest>